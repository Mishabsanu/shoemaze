<!-- Shop Cart Section Begin -->
<section class="shop-cart spad " style="text-align: center;">
    <div class="container-fluid">
        <h4>Wallet History</h4>
        <div class="row">
            <div class="col-lg-12">
                <div class="shop__cart__table">
                    <table>
                        <thead>
                            <tr>
                                <th>DATE</th>
                                <th>ORDER ID</th>
                                <th>Total Amount</th>
                                <th>STATUS</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#each user.walletHistory}}
                            <tr>
                                <td>{{date}}</td>
                                <td>{{orderId}}</td>
                                <td>â‚¹ {{amount}}</td>
                                <td>{{status}}</td>


                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-3">
            </div>
            <div class="col-md-3">
                <div class="cart__btn">
                    <a href="/">Continue Shopping</a>
                </div>
            </div>
            <div class="col-md-3">
            </div>
        </div>
    </div>
</section>
<!-- Shop Cart Section End -->

